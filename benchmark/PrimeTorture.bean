include_ffi "beanstd"
constant NUM <- 750000

function isPrime(n: integer) returns boolean
    for i <- 2 to integer(sqrt(n))
        if mod(n, i) = 0 then
            return false
        endif
    next i

    return true
endfunction

procedure primeTorture(n: integer)
    for i <- 1 to n
        if isPrime(i) then
            call Write(string(i) + ", ")
        endif
    next i
endprocedure

call primeTorture(NUM)
output ""
