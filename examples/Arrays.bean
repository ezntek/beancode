FUNCTION Predicate(x: INTEGER) RETURNS INTEGER
    RETURN 3 * x + 1
ENDFUNCTION

PROCEDURE PrintArray(arr: ARRAY[1:4] OF INTEGER)
    FOR i <- 1 TO 4
        OUTPUT arr[i]
    NEXT i
ENDPROCEDURE

DECLARE arr: ARRAY[1:4] OF INTEGER

FOR i <- 1 TO 4
    arr[i] <- Predicate(i)
NEXT i

CALL PrintArray(arr)
