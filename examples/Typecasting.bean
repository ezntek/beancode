// you can typecast variables as follows:

DECLARE Age_S: STRING
Age_S <- STRING("16")

// type casting is automatically done with INPUTs
DECLARE Price: STRING
OUTPUT "Enter the price of your good: "
INPUT Price

// calculate a discount
DECLARE PriceDiscount: REAL
PriceDiscount <- REAL(Price)
PriceDiscount <- PriceDiscount * (9/10)
OUTPUT "The price after a 90% discount is, ", ROUND(PriceDiscount, 2)
